    "version": "0.2.0",
    "configurations": [



        {
            "name": "Examples (Simple)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/examples/cbsw-unit-simple",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/examples",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },{
